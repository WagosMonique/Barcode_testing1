<?xml version="1.0" encoding="UTF-8"?><config>
  <kiosk>
    <application label="CpConcierge" package="com.datalogistics.cpconcierge" activity="com.datalogistics.cpconcierge.main.MainActivity"/>
  </kiosk>
  <applications>
    <application label="CpConcierge" package="com.datalogistics.cpconcierge" activity="com.datalogistics.cpconcierge.main.MainActivity"/>
  </applications>
  <tools>
    <application label="CpConcierge" package="com.datalogistics.cpconcierge" activity="com.datalogistics.cpconcierge.main.MainActivity"/>
  </tools>
  <passwords>
    <admin attempts="1"/>
  </passwords>
  <user_options/>
  <preferences>
    <orientation/>
    <wallpaper/>
    <usb_debugging_disabled>0</usb_debugging_disabled>
    <icon_label_text_color>#FFFFFFFF</icon_label_text_color>
